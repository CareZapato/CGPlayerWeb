{
  "name": "cgplayerweb",
  "version": "0.2.2",
  "description": "Aplicación web moderna para la gestión y reproducción de música coral",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/CareZapato/CGPlayerWeb.git"
  },
  "homepage": "https://github.com/CareZapato/CGPlayerWeb#readme",
  "bugs": {
    "url": "https://github.com/CareZapato/CGPlayerWeb/issues"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:network": "node scripts/start-with-ip.js && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev -- --host 0.0.0.0",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd backend && npm run build && cd ../frontend && npm run build",
    "start": "cd backend && npm start"
  },
  "keywords": ["music", "player", "choir", "web", "react", "nodejs", "postgresql", "audio", "streaming"],
  "author": {
    "name": "CareZapato",
    "email": "pbarrerap1988@gmail.com",
    "url": "https://github.com/CareZapato"
  },
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
